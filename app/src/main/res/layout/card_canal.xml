<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout android:layout_width="match_parent"
    android:layout_height="300dp"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_marginStart="15dp"
    android:layout_marginTop="5dp"
    android:layout_marginEnd="15dp"
    android:background="@drawable/viewholdercarnet_back"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    tools:ignore="MissingConstraints"
    tools:layout_marginTop="4dp"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout2"
        android:layout_width="346dp"
        android:layout_height="270dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="30dp"
        android:background="@drawable/viewholdercar_background"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/canalizacion2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="20dp"
            android:fontFamily="@font/handlee"
            android:text="CANALIZACIÓN"
            android:textAlignment="center"
            android:textColor="@color/azul02"
            android:textSize="18sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="7dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="7dp"
            app:layout_constraintTop_toBottomOf="@+id/canalizacion2">

            <TableRow>
                <TextView
                    android:layout_column="1"
                    android:background="@color/black"
                    android:padding="10dip"
                    android:fontFamily="@font/handlee"
                    android:textSize="12sp"
                    android:text="Área de Canalización"
                    android:textColor="@color/white" />
                <TextView
                    android:id="@+id/area_canalización"
                    android:padding="10dip"
                    android:textSize="12sp"
                    android:fontFamily="@font/handlee"
                    android:text= "Psicológia/Salud" />
            </TableRow>
            <TableRow>
                <TextView
                    android:layout_column="1"
                    android:background="@color/black"
                    android:padding="10dip"
                    android:fontFamily="@font/handlee"
                    android:textSize="12sp"
                    android:text="Fecha de Solicitud"
                    android:textColor="@color/white" />
                <TextView
                    android:id="@+id/fecha_solicitud"
                    android:padding="10dip"
                    android:fontFamily="@font/handlee"
                    android:textSize="12sp"
                    android:text="15/12/2023" />
            </TableRow>

            <TableRow>
                <TextView
                    android:layout_column="1"
                    android:background="@color/black"
                    android:padding="10dip"
                    android:textSize="12sp"
                    android:fontFamily="@font/handlee"
                    android:text="Nombre-Firma Tutor"
                    android:textColor="@color/white" />
                <TextView
                    android:id="@+id/nombrefirma_tutor1"
                    android:padding="10dip"
                    android:fontFamily="@font/handlee"
                    android:textSize="12sp"
                    android:text="Juan Carlos Madrigal Perez" />
            </TableRow>
            <TableRow>
                <TextView
                    android:layout_column="1"
                    android:background="@color/black"
                    android:padding="10dip"
                    android:textSize="12sp"
                    android:fontFamily="@font/handlee"
                    android:text="Observaciones"
                    android:textColor="@color/white" />
                <TextView
                    android:fontFamily="@font/handlee"
                    android:id="@+id/obs1"
                    android:padding="10dip"
                    android:textSize="12sp"
                    android:text="Nose" />
            </TableRow>
            <TableRow>
                <TextView
                    android:layout_column="1"
                    android:background="@color/black"
                    android:padding="10dip"
                    android:fontFamily="@font/handlee"
                    android:text="Fecha Atención"
                    android:textColor="@color/white" />
                <TextView
                    android:fontFamily="@font/handlee"
                    android:id="@+id/fecha_atencion1"
                    android:padding="10dip"
                    android:text="15/12/2023" />
            </TableRow>
        </TableLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>